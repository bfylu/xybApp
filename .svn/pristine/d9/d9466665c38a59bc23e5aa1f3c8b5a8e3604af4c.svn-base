<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_f2"
    android:orientation="vertical">

    <include
        android:id="@+id/include_title"
        layout="@layout/include_title"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y150" />

    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/swipeRefreshLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:scrollbars="none">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <RelativeLayout
                    style="@style/line"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/tv_pay_method"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/pay_method"
                        android:textSize="16sp" />

                    <ImageView
                        android:id="@+id/im_pay_icon"
                        android:layout_width="@dimen/x90"
                        android:layout_height="@dimen/y90"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_toRightOf="@id/tv_pay_method" />

                    <TextView
                        android:id="@+id/tv_pay_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_toRightOf="@id/im_pay_icon"
                        android:textSize="16sp" />
                </RelativeLayout>

                <RelativeLayout
                    style="@style/line"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/y40">

                    <TextView
                        android:id="@+id/door"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/door_man"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_door"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_toRightOf="@id/door"
                        android:textSize="14sp"
                        tools:text="" />
                </RelativeLayout>

                <RelativeLayout
                    style="@style/line"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/order"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/gather_money"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_order"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_toRightOf="@id/order"
                        android:textSize="14sp" />
                </RelativeLayout>

                <RelativeLayout
                    style="@style/line"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">


                    <TextView
                        android:id="@+id/Operator"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="操  作  员："
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_Operator"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_toRightOf="@+id/Operator"
                        tools:text="" />
                </RelativeLayout>

                <RelativeLayout
                    style="@style/line"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">


                    <TextView
                        android:id="@+id/tv_settle_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/pay_state"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_start_settle_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_toRightOf="@id/tv_settle_time"
                        android:textSize="14sp"
                        tools:text="" />
                </RelativeLayout>

                <RelativeLayout
                    style="@style/line"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">


                    <TextView
                        android:id="@+id/settle_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/pay_num"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/end_settle_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_toRightOf="@id/settle_time"
                        android:textSize="14sp"
                        tools:text="" />
                </RelativeLayout>


                <RelativeLayout
                    style="@style/line"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/total_recepits"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/pay_type"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_total_recepits"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_toRightOf="@id/total_recepits"
                        android:textSize="14sp"
                        tools:text="" />
                </RelativeLayout>

                <RelativeLayout
                    style="@style/line"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/label_prod_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="产品类型："
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_prod_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_toRightOf="@id/label_prod_type"
                        android:textSize="14sp"
                        tools:text="" />
                </RelativeLayout>

                <RelativeLayout
                    style="@style/line"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/total_refund"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/pay_organize"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_total_refund"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_toRightOf="@id/total_refund"
                        android:textSize="14sp"
                        tools:text="" />
                </RelativeLayout>

                <RelativeLayout
                    style="@style/line"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/total_deal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="@string/deal_time"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_total_deal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_toRightOf="@id/total_deal"
                        android:textSize="14sp"
                        tools:text="" />

                </RelativeLayout>

                <RelativeLayout
                    style="@style/line"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/tv_qian_danhao"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="交易单号："
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_danhao"
                        android:textIsSelectable="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/x40"
                        android:layout_toRightOf="@id/tv_qian_danhao"
                        android:textSize="14sp"
                        tools:text="" />
                </RelativeLayout>

                <Button
                    android:id="@+id/btn_choose"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/y30"
                    android:background="@drawable/selector_btn_refund"
                    android:text="@string/pay_logout"
                    android:textColor="@color/selected_color"
                    android:textSize="15sp"/>
            </LinearLayout>
        </ScrollView>
    </android.support.v4.widget.SwipeRefreshLayout>
</LinearLayout>
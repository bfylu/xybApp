<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="20dp"
    android:paddingRight="20dp"
    android:paddingTop="20dp"
    android:orientation="vertical">
    <TextView
        android:id="@+id/tv_mer_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="商户简称"
        android:textSize="17sp"
        android:textColor="@color/color_3"
        android:layout_gravity="center"
        android:layout_marginBottom="20dp"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp">
        <TextView
            android:id="@+id/lab_mer_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="商户编号："
            android:textSize="15sp"
            android:textColor="@color/color_6"
            />
        <TextView
            android:id="@+id/tv_mer_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="15sp"
            android:textColor="@color/color_3"
            android:layout_toRightOf="@+id/lab_mer_no"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp">
        <TextView
            android:id="@+id/lab_terminal_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="终  端  号："
            android:textSize="15sp"
            android:textColor="@color/color_6"
            />
        <TextView
            android:id="@+id/tv_terminal_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="15sp"
            android:textColor="@color/color_3"
            android:layout_toRightOf="@+id/lab_terminal_no"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp">
        <TextView
            android:id="@+id/lab_trade_amount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="交易金额："
            android:textSize="15sp"
            android:textColor="@color/color_6"
            />
        <TextView
            android:id="@+id/tv_trade_amount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="15sp"
            android:textColor="@color/color_3"
            android:layout_toRightOf="@+id/lab_trade_amount"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp">
        <TextView
            android:id="@+id/lab_pay_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="支付类型："
            android:textSize="15sp"
            android:textColor="@color/color_6"
            />
        <TextView
            android:id="@+id/tv_pay_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="15sp"
            android:textColor="@color/color_3"
            android:layout_toRightOf="@+id/lab_pay_type"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_orig_order_no"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:visibility="gone">
        <TextView
            android:id="@+id/lab_ass_trade_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="关联单号："
            android:textSize="15sp"
            android:textColor="@color/color_6"
            />
        <TextView
            android:id="@+id/tv_ass_trade_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="15sp"
            android:textColor="@color/color_3"
            android:layout_toRightOf="@+id/lab_ass_trade_no"/>
    </RelativeLayout>
    <RelativeLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp">
        <TextView
            android:id="@+id/lab_trade_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="交易单号："
            android:textSize="15sp"
            android:textColor="@color/color_6"
            />
        <TextView
            android:id="@+id/tv_trade_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="15sp"
            android:textColor="@color/color_3"
            android:layout_toRightOf="@+id/lab_trade_no"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp">
        <TextView
            android:id="@+id/lab_trade_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="交易日期："
            android:textSize="15sp"
            android:textColor="@color/color_6"
            />
        <TextView
            android:id="@+id/tv_trade_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="15sp"
            android:textColor="@color/color_3"
            android:layout_toRightOf="@+id/lab_trade_time"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp">
        <TextView
            android:id="@+id/lab_shop_site"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="店铺地址："
            android:textSize="15sp"
            android:textColor="@color/color_6"
            />
        <TextView
            android:id="@+id/tv_shop_site"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="15sp"
            android:textColor="@color/color_3"
            android:layout_toRightOf="@+id/lab_shop_site"/>

    </RelativeLayout>
    <View
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:layerType="software"
        android:background="@drawable/border_dotted_line"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp">
        <TextView
            android:id="@+id/lab_technical_support"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="技术支持："
            android:textSize="15sp"
            android:textColor="@color/color_3"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PAY+"
            android:textSize="15sp"
            android:textColor="@color/color_3"
            android:layout_toRightOf="@+id/lab_technical_support"/>

    </RelativeLayout>
